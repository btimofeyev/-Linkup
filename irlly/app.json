{
  "expo": {
    "name": "Linkup",
    "slug": "linkup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/linkuplogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF8F0"
    },
    "ios": {
      "bundleIdentifier": "com.linkup.app",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/linkuplogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF8F0",
        "tabletImage": "./assets/linkuplogo.png"
      },
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to help you find friends who are also using the app and create friend circles.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to create pins and meetups at your current location.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to create pins and meetups at your current location."
      }
    },
    "android": {
      "package": "com.linkup.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/linkuplogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF8F0"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts to help you find friends and create circles."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}